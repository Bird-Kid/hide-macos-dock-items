<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>

    <key>Label</key>
    <string>local.bird.HideDockItems.Trash</string>

    <key>Program</key>
    <string>/usr/bin/osascript</string>

    <key>ProgramArguments</key>
    <array>
      <string>osascript</string>
      <string>/path/to/hide_macos_dock_items.applescript</string>
      <string>trash</string>
    </array>

    <key>RunAtLoad</key>
    <true/>

    <key>StandardErrorPath</key>
    <string>/tmp/local.bird.HideDockItems.Trash.err.log</string>

  </dict>
</plist>
